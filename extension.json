{
	"name": "Graph",
	"author": [
		"Dan Andreescu",
		"Yuri Astrakhan",
		"Frédéric Bolduc"
	],
	"url": "https://www.mediawiki.org/wiki/Extension:Graph",
	"descriptionmsg": "graph-desc",
	"license-name": "MIT",
	"type": "other",
	"MessagesDirs": {
		"Graph": [
			"i18n"
		]
	},
	"AutoloadClasses": {
		"Graph\\ApiGraph": "ApiGraph.php",
		"Graph\\Singleton": "Graph.body.php",
		"Graph\\Content": "Graph.body.php",
		"GraphHooks": "Graph.hooks.php"
	},
	"APIModules": {
		"graph": "Graph\\ApiGraph"
	},
	"ResourceModules": {
		"ext.graph.loader": {
			"scripts": [
				"modules/graph-loader.js"
			],
			"styles": [
			  "styles/common.less"
			],
			"messages": [
				"graph-loading",
				"graph-loading-done"
			],
			"dependencies": [
				"mediawiki.Uri"
			],
			"targets": [
				"mobile",
				"desktop"
			]
		},
		"ext.graph": {
			"styles": [
				"styles/common.less"
			],
			"dependencies": [
				"mediawiki.Uri"
			],
			"targets": [
				"mobile",
				"desktop"
			]
		},
		"ext.graph.vega1": {
			"scripts": [
				"lib/d3.js",
				"lib/topojson.js",
				"lib/vega1/vega.js",
				"modules/graph1.js"
			],
			"dependencies": [
				"ext.graph"
			],
			"targets": [
				"mobile",
				"desktop"
			]
		},
		"ext.graph.vega2": {
			"scripts": [
				"lib/d3.js",
				"lib/topojson.js",
				"lib/vega2/vega.js",
				"modules/graph2.js"
			],
			"dependencies": [
				"ext.graph"
			],
			"targets": [
				"mobile",
				"desktop"
			]
		},
		"ext.graph.editor": {
			"scripts": [
				"modules/graph.editor.js"
			],
			"targets": [
				"mobile",
				"desktop"
			]
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "",
		"remoteExtPath": "Graph"
	},
	"Hooks": {
		"ParserFirstCallInit": [
			"Graph\\Singleton::onParserFirstCallInit"
		],
		"EditPage::showEditForm:initial": [
			"Graph\\Singleton::editPageShowEditFormInitial"
		],
		"ParserAfterParse": [
			"Graph\\Singleton::onParserAfterParse"
		],
		"ResourceLoaderRegisterModules": [
			"GraphHooks::onResourceLoaderRegisterModules"
		],
		"ResourceLoaderTestModules": [
			"GraphHooks::onResourceLoaderTestModules"
		]
	},
	"VisualEditorPluginModules": [
		"ext.graph.visualEditor"
	],
	"config": {
		"GraphDataDomains": [],
		"GraphDefaultVegaVer": 1,
		"GraphUrlBlacklist": false,
		"GraphIsTrusted": false,
		"GraphImgServiceUrl": false
	},
	"TrackingCategories": [
		"graph-tracking-category",
		"graph-broken-category"
	],
	"manifest_version": 1
}
